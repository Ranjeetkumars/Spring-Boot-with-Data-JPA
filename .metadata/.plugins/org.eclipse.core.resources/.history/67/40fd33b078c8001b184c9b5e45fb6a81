package com.example;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

@Configuration
public class Test {
	
	@Bean
	public A a() {
		A a1 = new A();
		return  a1;
	}

}




class A {
	
	public A() {
		
		System.out.println("@@@@@@@@@@@@@@@@@@@@@@@@@");
	}
	
	
}
